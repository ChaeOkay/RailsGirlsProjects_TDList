<tbody>
  <div class="new">
	 <%= link_to 'Create New List', new_list_path %>
  </div>

  <div>
    <% @lists.each do |list| %>
      <h3>
        <span style="color:#999999"><%= list.id %></span>  
        <%= link_to list.name, task_path(list.id) %> 
        <small style="color:#999999"><%=list.list_status%></small>
      </h3>
     <% end %>
  </div>
</tbody>